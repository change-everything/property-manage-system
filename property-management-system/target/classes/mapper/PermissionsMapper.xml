<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.f4.mapper.PermissionsMapper">


    <select id="selectListByPath" resultType="permissions">
        select *
        from permissions
        where url = #{url};
    </select>


</mapper>